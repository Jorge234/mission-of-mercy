<h1>Listing procedure_tooth_numbers</h1>

<table>
  <tr>
    <th>Procedure</th>
    <th>Tooth number</th>
  </tr>

<% for procedure_tooth_number in @procedure_tooth_numbers %>
  <tr>
    <td><%=h procedure_tooth_number.procedure_id %></td>
    <td><%=h procedure_tooth_number.tooth_number %></td>
    <td><%= link_to 'Show', procedure_tooth_number %></td>
    <td><%= link_to 'Edit', edit_procedure_tooth_number_path(procedure_tooth_number) %></td>
    <td><%= link_to 'Destroy', procedure_tooth_number, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New procedure_tooth_number', new_procedure_tooth_number_path %>
