<h1>New patient_procedure</h1>

<% form_for(@patient_procedure) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :patient_id %><br />
    <%= f.text_field :patient_id %>
  </p>
  <p>
    <%= f.label :procedure_id %><br />
    <%= f.text_field :procedure_id %>
  </p>
  <p>
    <%= f.label :tooth_number %><br />
    <%= f.text_field :tooth_number %>
  </p>
  <p>
    <%= f.label :surface_code %><br />
    <%= f.text_field :surface_code %>
  </p>
  <p>
    <%= f.label :provider %><br />
    <%= f.text_field :provider %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', patient_procedures_path %>
